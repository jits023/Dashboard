<?xml version="1.0" encoding="UTF-8" standalone="no"?><DATASOURCE name="classicmodels1.4" sourcetype="jdbc" type="mysql"><DATASET isStoredProc="false" name="Sales" type="NormalDataset"><QUERY>select customers.country,lower(customers.countrycode) as countrycode,orders.orderDate,orderdetails.productCode,orderdetails.quantityOrdered,orderdetails.priceEach,orderdetails.orderNumber,
